<section>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <%= render "date_pills", first_iteration: @first_iteration, default_weeks: 10, path: :charts_long_term_trend_path %>
      </div>
      <%= render "long_term_trend_stats", stats: @stats, colspan: 3 %>
    </div>
    <%= render "long_term_trend_chart", chart: @long_term_trend_chart %>
  </div>
</section>